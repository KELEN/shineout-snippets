{
  "name": "shineout-snippets",
  "displayName": "shineout-snippets",
  "description": "shineout happy coding",
  "publisher": "kelen",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
		"*"
  ],
  "main": "./out/extension",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "vue",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "@types/vscode": "^1.41.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
