  "activationEvents": [
    "onCommand:shineout.completion",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onCommand:shineout.docs",
    "onCommand:shineout.button",
    "onCommand:shineout.dropdown",
    "onCommand:shineout.icon",
    "onCommand:shineout.image",
    "onCommand:shineout.cascader",
    "onCommand:shineout.checkbox",
