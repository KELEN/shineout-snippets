{
  "name": "shineout-snippets",
  "displayName": "shineout-snippets",
  "description": "shineout happy coding",
  "publisher": "kelen",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.41.0"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "api": "node ./src/translateApi",
    "snippets": "node ./src/translateSnippets"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "@types/vscode": "^1.41.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "lodash": "^4.17.15",
    "marked": "^0.8.0"
  }
}
