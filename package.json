{
  "name": "shineout-snippets",
  "displayName": "shineout-snippets",
  "description": "shineout happy coding",
  "publisher": "kelen",
  "version": "0.0.61",
  "repository": "https://github.com/KELEN/shineout-snippets",
  "engines": {
    "vscode": "^1.41.0"
  },
  "icon": "images/icon.png",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "api": "node ./src/translateApi",
    "snippets": "node ./src/translateSnippets"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:shineout.completion",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onCommand:shineout.docs"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "shineout.docs",
        "title": "shineout文档"
      },
      {
        "command": "shineout.completion",
        "title": "shineout自动提示"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "isWindows || isMac",
          "command": "shineout.docs",
          "group": "navigation"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "@types/vscode": "^1.41.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-traverse": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.19",
    "marked": "^0.8.0",
    "prettier": "^1.19.1"
  }
}
